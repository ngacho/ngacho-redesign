<!DOCTYPE html>
<html lang="en">

<head>
    <link href="/assets/style/admin/admin-home.css" rel="stylesheet" type="text/css">

    <%- include('../partials/head'); %>
</head>

<body class="container">

    <header>
        <%- include('../partials/navigation-bar'); %>
    </header>


    <main>
        <div class="dash-board-container">

            <% for (let card of adminCards){ %>

                <a class="action-card" href="<%= card.link %>">
                    <h1 class="action-prompt action-details">
                        <%= card.title %>
                    </h1>
                </a>

            <% } %>


        </div>


    </main>

</body>

</html>